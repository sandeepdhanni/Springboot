    <bpmn:serviceTask id="send_event_joke" name="Send Joke" zeebe:modelerTemplate="io.camunda.connectors.SendGrid.v2" zeebe:modelerTemplateVersion="3" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:sendgrid:1" retries="3" />
