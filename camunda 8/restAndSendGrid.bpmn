          <zeebe:input source="text/plain" target="content.type" />
          <zeebe:input source="=setup + &#34; &#34; + punchline" target="content.value" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
